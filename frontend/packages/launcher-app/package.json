{
  "name": "@launcher/app",
  "version": "1.0.0",
  "private": true,
  "homepage": "/launch/",
  "license": "Apache-2.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:mock-api": "env-cmd .env.mock-api yarn build",
    "test": "jest",
    "eject": "react-scripts eject",
    "start:mock-api": "env-cmd .env.mock-api yarn start",
    "start:staging-api": "env-cmd .env.staging-api yarn start",
    "start:local-api": "env-cmd .env.local-api yarn start",
    "start:production-api": "env-cmd .env.production-api yarn start",
    "start:prod": "run-s build serve",
    "start:custom": "env-cmd .env.custom yarn start",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "storybook:build": "build-storybook -s public",
    "storybook:publish": "npx surge --project ./storybook-static --domain https://launcher-storybook.surge.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@launcher/component": "2.0.2",
    "@sentry/browser": "5.5.0",
    "project-name-generator": "2.1.7",
    "axios": "0.19.0",
    "jssha": "2.3.1",
    "keycloak-js": "6.0.1",
    "query-string": "6.8.1",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2",
    "lscache": "1.3.0",
    "reflect-metadata": "0.1.13",
    "injection-js": "2.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addon-storysource": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@storybook/storybook-deployer": "2.8.1",
    "@storybook/theming": "5.1.9",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__react": "4.0.2",
    "@types/lscache": "1.0.29",
    "@types/jssha": "2.0.0",
    "@types/react-router-dom": "4.3.4",
    "@types/uuid": "3.4.5",
    "axios-mock-adapter": "1.17.0",
    "env-cmd": "9.0.3",
    "react-scripts": "3.0.1"
  }
}
